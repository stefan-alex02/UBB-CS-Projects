<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ir.map.g221.guisocialnetwork.controllers.userperspective.FriendListController">
    <TableView fx:id="tableView" prefHeight="150.0" prefWidth="300.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columns>
            <TableColumn fx:id="tableColumnFirstName" prefWidth="100.0" styleClass="user-table-column" text="First name" />
            <TableColumn fx:id="tableColumnLastName" prefWidth="100.0" styleClass="user-table-column" text="Last name" />
            <TableColumn fx:id="tableColumnFriendsFrom" prefWidth="100.0" styleClass="user-table-column" text="Friends count" />
            <TableColumn fx:id="tableColumnMessageFriend" prefWidth="75.0" styleClass="user-table-column" />
            <TableColumn fx:id="tableColumnRemoveFriend" prefWidth="75.0" styleClass="user-table-column" />
        </columns>
      <columnResizePolicy>
         <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
      </columnResizePolicy>
    </TableView>
</AnchorPane>
