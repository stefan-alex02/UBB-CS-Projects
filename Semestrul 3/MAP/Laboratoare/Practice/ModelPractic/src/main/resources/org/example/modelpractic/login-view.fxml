  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <children>
      <TextField fx:id="textFieldUsername" alignment="CENTER" prefHeight="31.0" promptText="Username" />
      <Button onAction="#onLoginButtonClick" mnemonicParsing="false" prefHeight="25.0" prefWidth="86.0" text="Login" />
   </children>
</VBox>
